<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="org.eclipse.e4.babel.editor.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="babel.translator.editor.e4xmi">
      </fragment>
   </extension>
   <extension
         id="org.eclipse.e4.babel.editor.toolbar"
         name="Toolbar Core Expression"
         point="org.eclipse.core.expressions.definitions">
   </extension>
   <extension
         point="at.nucle.e4.plugin.preferences">
      <page
            class="org.eclipse.e4.babel.editor.preference.PreferenceGeneralPage"
            id="page_babel_general"
            order="1">
      </page>
      <page
            category="page_babel_general"
            class="org.eclipse.e4.babel.editor.preference.PreferenceFormattingPage"
            id="page_babel_formatting"
            order="2">
      </page>
      <page
            category="page_babel_general"
            class="org.eclipse.e4.babel.editor.preference.PreferencePerformancePage"
            id="page_babel_performance"
            order="3">
      </page>
   </extension>


</plugin>
